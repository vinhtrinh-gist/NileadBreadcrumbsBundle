<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <!-- Generate product's category and all parent categories -->
        <parameter key="nilead.resolver.category_tree">CategoryTree</parameter>

        <!-- Generate product's category -->
        <parameter key="nilead.resolver.category">Category</parameter>

        <!-- Generate product item -->
        <parameter key="nilead.resolver.product">Product</parameter>
    </parameters>

</container>
